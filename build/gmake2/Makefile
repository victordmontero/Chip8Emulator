# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_windows
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_windows)
  Chip8Emulador_config = debug_windows
endif
ifeq ($(config),debug_linux)
  Chip8Emulador_config = debug_linux
endif
ifeq ($(config),release_windows)
  Chip8Emulador_config = release_windows
endif
ifeq ($(config),release_linux)
  Chip8Emulador_config = release_linux
endif

PROJECTS := Chip8Emulador

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Chip8Emulador:
ifneq (,$(Chip8Emulador_config))
	@echo "==== Building Chip8Emulador ($(Chip8Emulador_config)) ===="
	@${MAKE} --no-print-directory -C . -f Chip8Emulador.make config=$(Chip8Emulador_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Chip8Emulador.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_windows"
	@echo "  debug_linux"
	@echo "  release_windows"
	@echo "  release_linux"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Chip8Emulador"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"